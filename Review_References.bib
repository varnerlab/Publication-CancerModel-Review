	Pages = {ra68},
	Pmid = {23943608},
	Pst = {epublish},
	Title = {Computational modeling of ERBB2-amplified breast cancer identifies combined ErbB2/3 blockade as superior to the combination of MEK and AKT inhibitors},
	Volume = {6},
