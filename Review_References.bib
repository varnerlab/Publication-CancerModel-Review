	Journal = {PLoS One},
	Journal-Full = {PloS one},
	Mesh = {Algorithms; Animals; Cadherins; Cell Proliferation; Computer Simulation; Humans; Models, Biological; Monte Carlo Method; Neoplasm Invasiveness; Neoplasms; Spheroids, Cellular; beta Catenin},
	Number = {3},
